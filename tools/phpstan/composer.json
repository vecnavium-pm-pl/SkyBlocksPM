{
    "name": "vecnavium/skyblockspm",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sof3/libasynql",
                "version": "4.1.1",
                "source": {
                    "url": "https://github.com/alvin0319/libasynql.git",
                    "type": "git",
                    "reference": "pm5"
                },
                "autoload": {
                    "psr-0": {
                        "poggit\\libasynql\\": "libasynql/src/"
                    }
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "sof3/infoapi",
                "version": "1.3.0",
                "source": {
                    "url": "https://github.com/DuoIncure/InfoAPI.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0": {
                        "SOFe\\InfoAPI\\": "src/"
                    }
                }
            }
        }
    ],
    "require": {
        "phpstan/phpstan": "1.4.6",
        "pocketmine/pocketmine-mp": "5.0.0-BETA2",
        "phpstan/phpstan-strict-rules": "^1.0",
        "phpstan/extension-installer": "^1.0",
        "duoincure/commando": "^1.0",
        "sylvrs/libmarshal": "^1.4.3",
        "sof3/libasynql": "*",
        "sof3/infoapi": "*"
    },
    "autoload": {
        "psr-0": {
            "Vecnavium\\SkyBlocksPM\\": "../../src/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
